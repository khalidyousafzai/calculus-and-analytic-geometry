\draw(0,0)--++(\ang:\r)coordinate(ka)node[pos=0.7,left,yshift=1ex]{$\sqrt{x^2+4}$}--($(0,0)!(ka)!(2,0)$)node[pos=0.5,right]{$x$}--(0,0)node[pos=0.5,below]{$2$};
\draw([shift={(0:0.5)}]0,0) arc (0:\ang:0.5);
\draw(1/2*\ang:0.7)node[]{$\theta$};
