\addplot[fill=lgray]fill between[of=f and ttop];
\draw(axis cs:pi,2)--(axis cs:pi,0)node[pos=0.5,sloped,above]{$x=\pi$};
\end{axis}
\end{tikzpicture}
\caption{}
\label{شکل_سوال_تکمل_رقبہ_سایہ_دار_الف}
