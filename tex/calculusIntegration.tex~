\draw[dashed](\b:1.25cm and 0.5cm)coordinate(kBB)--++(0,\h)coordinate(kB);
\draw(-180+\a:1.25cm and 0.5cm)coordinate(kCC)--++(0,\h)coordinate(kC);
\draw(-180+\b:1.25cm and 0.5cm)coordinate(kDD)--++(0,\h)coordinate(kD);
\draw(kA)--(kD);
\draw(kB)--(kC);
