\draw(1,-2)node[]{$x^2=y$};
\draw(5,0)node[below]{$5$};
\draw[gray](4-0.2,0)--++(0.4,0);
\draw[stealth-stealth,gray](4,0)node[circ]{}node[below]{$4$}--++(0,2)node[pos=0.5,fill=white]{$4$};
\end{tikzpicture}
\caption{راکٹ کی نوک (سوال \حوالہ{سوال_تکمل_راکٹ_نوک})}
